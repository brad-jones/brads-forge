{
  "incremental": true,
  "lineWidth": 120,
  "newLineKind": "lf",
  "useTabs": false,
  "json": {},
  "toml": {},
  "typescript": {
    "deno": true,
    "lineWidth": 120,
    "exportDeclaration.preferSingleLine": true,
    "exportDeclaration.sortNamedExports": "maintain",
    "ignoreFileCommentText": "dprint-ignore-file",
    "ignoreNodeCommentText": "dprint-ignore",
    "importDeclaration.preferSingleLine": true,
    "importDeclaration.sortNamedImports": "maintain",
    "module.sortExportDeclarations": "maintain",
    "module.sortImportDeclarations": "maintain"
  },
  "markdown": {
    "textWrap": "always"
  },
  "dockerfile": {},
  "prettier": {
    "associations": [
      "**/*.{yml,yaml}"
    ]
  },
  "exec": {
    "commands": [
      {
        "command": "shfmt",
        "exts": [
          "sh"
        ]
      }
    ]
  },
  "includes": [
    "**/*.{json,md,dockerfile,yml,yaml,toml,sh,ts}"
  ],
  "excludes": [
    "**/.pixi",
    "pixi.lock"
  ],
  "plugins": [
    "https://plugins.dprint.dev/toml-0.6.3.wasm",
    "https://plugins.dprint.dev/json-0.19.4.wasm",
    "https://plugins.dprint.dev/markdown-0.17.8.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.2.wasm",
    "https://plugins.dprint.dev/typescript-0.93.3.wasm",
    "https://plugins.dprint.dev/exec-0.5.1.json@492414e39dea4dccc07b4af796d2f4efdb89e84bae2bd4e1e924c0cc050855bf",
    "https://plugins.dprint.dev/prettier-0.46.1.json@e5bd083088a8dfc6e5ce2d3c9bee81489b065bd5345ef55b59f5d96627928b7a"
  ]
}
